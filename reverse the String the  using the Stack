#include <iostream>
#include<stack>
using namespace std;

int main() {
  cout << "Hello world!" << endl;

   string str = "SurajSanyal";
   stack<char>s;

  //  for(int i =1 ; i)
  
  int i =0;
  while(i<str.length()){
     s.push(str[i]);
     i++;
  }

  while(s.size()!=0){
     cout<<s.top()<<" ";
     s.pop();
  }


  return 0;
}
